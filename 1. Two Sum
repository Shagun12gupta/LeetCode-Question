class Solution {
    public int[] twoSum(int[] nums, int target) {
        int a =nums.length;
        int ans[] = new int[2];
        int sum=0;
        for (int i=0; i<a;i++ ){
            for (int j=i+1; j<a;j++){
                sum = nums[i]+nums[j];
                if (sum==target){ 
                    ans[0]= i;
                    ans[1]=j;
                }
            }
        }
    return ans;
    }
}
