class Solution {
    public int averageValue(int[] nums) {
        int sum=0, count=0;
        for(int i =0; i< nums.length; i++){
            if(nums[i]%3==0 && nums[i]%2==0){
                sum = sum+nums[i];
                count++;
            }
        }
        if (count>0){
        int avg = sum/count;
        return avg;}
        else{
            return 0;
        }
    }
}
